<%= simple_form_for(@manufactoring) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :batch_amount %>
    <%= f.input :batch_unit %>
    <%= f.input :batch_date %>
    <%= f.input :batch_status %>
    <%= f.association :order %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
