<%- model_class = Manufactoring -%>
<div class="page-header">
  <h1><%=t '.title', :default => Manufactoring %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:batch_amount) %></dt>
      <dd><pre class="prettyprint"><%= @manufactoring.batch_amount %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:batch_unit) %></dt>
      <dd><pre class="prettyprint"><%= @manufactoring.batch_unit %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:batch_date) %></dt>
      <dd><pre class="prettyprint"><%= @manufactoring.batch_date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:batch_status) %></dt>
      <dd><pre class="prettyprint"><%= @manufactoring.batch_status %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              manufactorings_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_manufactoring_path(@manufactoring), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              manufactoring_path(@manufactoring),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

